  "keywords": [
    "react",
    "elf",
    "button"
  ],
  "author": "elf-framework",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@react-elf-types/alert": "*",
    "@react-elf/shared": "*",
    "@types/react": "^18.0.26",
    "classnames": "^2.3.2",
    "react": "^16.8.0 || ^17.0.0-rc.1 || ^18.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@vitejs/plugin-react": "^3.0.0",
    "chokidar-cli": "^3.0.0",
    "nanobundle": "^1.1.0",
    "vite": "^4.0.3",
    "vitest": "0.26.2"
  }
