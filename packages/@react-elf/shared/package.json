{
  "name": "@react-elf/shared",
  "version": "0.0.26",
  "description": "",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "npm-check": "npx npm-check-updates -u",
    "vite:dist": "vite build --config=vite.dist.config.js",
    "watch": "chokidar \"src/**/*\" -c \"pnpm run nano:build\" --polling",
    "vite:watch": "vite build --config=vite.dist.config.js --watch",
    "ui:watch": "pnpm run watch",
    "docs:dev": "pnpm run ui:watch",
    "build": "pnpm run nano:build",
    "nano:build": "nanobundle build",
    "test": "vitest",
    "coverage": "vitest --coverage"
  },
  "keywords": [
    "react",
    "elf",
    "button"
  ],
  "author": "elf-framework",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@react-elf-types/shared": "workspace:*",
    "@types/react": "^18.0.26",
    "classnames": "^2.3.2",
    "react": "^16.8.0 || ^17.0.0-rc.1 || ^18.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "chokidar-cli": "^3.0.0",
    "nanobundle": "^1.6.0",
    "vite": "^4.3.9",
    "vitest": "0.33.0"
  }
}
